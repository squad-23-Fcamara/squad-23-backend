generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model public_schedules {
  id           String         @id @default(uuid())
  schedule_to  DateTime
  created_at   DateTime       @default(now())
  public_users public_users[] @relation("public_scheduleTopublic_user")
}

model public_users {
  id               String             @id @default(uuid())
  name             String
  email            String             @unique
  password         String
  skills           String[]
  mentor           Boolean
  public_schedules public_schedules[] @relation("public_scheduleTopublic_user")
}
